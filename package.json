{
  "name": "6tisch-simulator-webapp",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "concurrently \"npm:backend-dev-server\" \"npm:dev-server\"",
    "build": "vue-cli-service build",
    "test": "concurrently -k -s first \"npm:backend-dev-server\" \"npm:test:unit\"",
    "lint": "vue-cli-service lint",
    "backend-dev-server": "backend/start --auto-restart --dev",
    "dev-server": "vue-cli-service serve",
    "test:backend": "pytest backend",
    "test:unit": "vue-cli-service test:unit",
    "deploy": "npm run build && ./deploy.py"
  },
  "dependencies": {
    "apexcharts": "^3.24.0",
    "concurrently": "^4.1.0",
    "cytoscape-dagre": "^2.2.2",
    "two.js": "^0.7.0-beta.3",
    "vue": "^2.6.6",
    "vue-apexcharts": "^1.2.9",
    "vue-cytoscape": "^0.1.2",
    "vue-router": "^3.0.1",
    "vuetify": "^1.5.2",
    "vuex": "^3.1.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.4.1",
    "@vue/cli-plugin-eslint": "^3.4.1",
    "@vue/cli-plugin-unit-jest": "^3.4.1",
    "@vue/cli-service": "^3.4.1",
    "@vue/test-utils": "^1.0.0-beta.29",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "eslint": "^5.14.1",
    "eslint-plugin-vue": "^5.2.2",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "vue-cli-plugin-vuetify": "^0.4.6",
    "vue-template-compiler": "^2.6.6",
    "vuetify-loader": "^1.2.1"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
